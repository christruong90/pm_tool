<h1> search page </h1>

<table>
  <thead>
  <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Due date</th>
    </tr>
  </thead>
  <tbody>
    <% @projects.each do |project| %>
      <tr>
        <td><%= project.title %></td>
        <td><%= project.description %></td>
        <td><%#= project.due_date %></td>
        <td><%#= link_to "Edit", edit_project_path(project) %> <%#= link_to "Delete", request, method: :delete, data: {confirm: "Are you sure?"} %></td>
      </tr>
    <% end %>
  </tbody>

</table>
